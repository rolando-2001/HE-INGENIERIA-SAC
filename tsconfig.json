{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@sections/*": [
        "src/sections/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@data/*": [
        "src/data/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
     
  }
 
}
